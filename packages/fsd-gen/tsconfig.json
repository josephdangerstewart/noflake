{
  "extends": "@noflake/typescript-config/cross-platform.json",
  "include": ["."],
  "exclude": ["node_modules", "dist/**"],
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noImplicitAny": false
  }
}
